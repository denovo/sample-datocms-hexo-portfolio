<article class="sheet">
  <div class="sheet__inner">
    <h1 class="sheet__title"><%= page.title %></h1>
    <p class="sheet__lead"><%= page.subtitle %></p>
    <div class="sheet__gallery">
      <img src="<%= page.photo %>" />
    </div>
    <div class="sheet__body">
      <%- page.content %>
    </div>
  </div>
</article>

